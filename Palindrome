#include <iostream>
#include <stdio.h>
#include <string>

using namespace std;

string input;
int len;

bool IsPalindrome(string s)
{
	int start=0,end=len-1;

	while(start<end)
	{
		if(s[start]!=s[end])
			return false;

		start++;
		end--;
	}

	return true;
}

string can(int i)
{
	string cur=input.substr(i,len)+input.substr(0,i); // reverse concatenate

	if(IsPalindrome(cur)) // check if is palindrome
		return cur;

	return "";
}

int main()
{
	cin >> input;
	len=input.length();

	int count=0;
	for(int i=0;i<len;i++)
	{
		string out=can(i);
		if(out!="")
		{
			printf("%s\n",out.c_str());
//			cout << out << endl;
			count ++;
		}
	}

	if(!count) // if no palindromes found
		cout << "NONE" << endl;

  return 0;
}
